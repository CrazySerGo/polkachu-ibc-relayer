[[chains]]
id = 'qsr-questnet-04'
rpc_addr = 'http://{{ testnet_quasar_ip }}:18257'
grpc_addr = 'http://{{ testnet_quasar_ip }}:18290'
websocket_addr = 'ws://{{ testnet_quasar_ip }}:18257/websocket'

rpc_timeout = '20s'
account_prefix = 'quasar'
key_name = '{{ key_name }}'
store_prefix = 'ibc'
address_type = { derivation = 'cosmos' }
default_gas = 300000
max_gas = 75000000
gas_price = { price = 0.025, denom = 'uqsr' }
gas_multiplier = 1.3
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '2days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
    ['transfer', 'channel-0'], # Quasar-Osmosis
    ['wasm.quasar1nc5tatafv6eyq7llkr2gv50ff9e22mnf70qgjlv737ktmt4eswrqgsk7e6', '*'],
    ['wasm.quasar1wug8sewp6cedgkmrmvhl3lf3tulagm9hnvy8p0rppz9yjw0g4wtqu5prdk', '*'],
    ['wasm.quasar1xr3rq8yvd7qplsw5yx90ftsr2zdhg4e9z60h5duusgxpv72hud3syhezhc', '*'],
]
