[[chains]]
id = 'osmo-test-4'
rpc_addr = 'http://{{ testnet_osmosis_ip }}:12557'
grpc_addr = 'http://{{ testnet_osmosis_ip }}:12590'
websocket_addr = 'ws://{{ testnet_osmosis_ip }}:12557/websocket'

rpc_timeout = '20s'
account_prefix = 'osmo'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 500000
max_gas = 120000000
gas_price = { price = 0.000, denom = 'uosmo' }
gas_multiplier = 1.8
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '7days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', 'channel-312'], # Axelar
]
