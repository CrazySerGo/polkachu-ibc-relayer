[[chains]]
id = 'atlantic-1'
rpc_addr = 'http://{{ testnet_sei_ip }}:11957'
grpc_addr = 'http://{{ testnet_sei_ip }}:11990'
websocket_addr = 'ws://{{ testnet_sei_ip }}:11957/websocket'

rpc_timeout = '20s'
account_prefix = 'sei'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 500000
max_gas = 120000000
gas_price = { price = 0.000, denom = 'usei' }
gas_multiplier = 1.8
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '2days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }
