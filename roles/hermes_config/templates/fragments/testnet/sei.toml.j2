[[chains]]
id = 'atlantic-2'
rpc_addr = 'http://{{ testnet_sei_ip }}:11957'
grpc_addr = 'http://{{ testnet_sei_ip }}:11990'
websocket_addr = 'ws://{{ testnet_sei_ip }}:11957/websocket'

rpc_timeout = '20s'
account_prefix = 'sei'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 300000
max_gas = 10000000
gas_price = { price = 0.01, denom = 'usei' }
gas_multiplier = 1.8
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '2days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '2', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', 'channel-29'], # Axelar
  ['transfer', 'channel-43'], # Osmosis
]