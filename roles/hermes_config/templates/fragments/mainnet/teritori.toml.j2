[[chains]]
id = 'teritori-1'
rpc_addr = 'http://{{ teritori_ip }}:15957'
grpc_addr = 'http://{{ teritori_ip }}:15990'
websocket_addr = 'ws://{{ teritori_ip }}:15957/websocket'

rpc_timeout = '20s'
account_prefix = 'tori'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 300000
max_gas = 35000000
gas_price = { price = 0.000, denom = 'utori' }
gas_multiplier = 1.2
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '7days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }
