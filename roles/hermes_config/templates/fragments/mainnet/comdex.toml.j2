[[chains]]
id = 'comdex-1'
rpc_addr = 'http://{{ comdex_ip }}:13157'
grpc_addr = 'http://{{ comdex_ip }}:13190'
websocket_addr = 'ws://{{ comdex_ip }}:13157/websocket'

rpc_timeout = '20s'
account_prefix = 'comdex'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 500000
max_gas = 1000000
gas_price = { price = 0.025, denom = 'ucmdx' }
gas_multiplier = 1.8
max_msg_num = 30
max_tx_size = 2097152
clock_drift = '1000s'
max_block_time = '10s'
trusting_period = '14days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', 'channel-52'], # Akash
  ['transfer', 'channel-34'], # Axelar
  ['transfer', 'channel-61'], # Chihuahua
  ['transfer', 'channel-37'], # Cosmos
  ['transfer', 'channel-49'], # Crescent
  ['transfer', 'channel-35'], # Evmos
  ['transfer', 'channel-23'], # Gravity
  ['transfer', 'channel-18'], # Juno
  ['transfer', 'channel-31'], # Kujira
  ['transfer', 'channel-1'], # Osmosis
  ['transfer', 'channel-57'], # Persistence
]