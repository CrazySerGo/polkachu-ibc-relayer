[[chains]]
id = 'fetchhub-4'
rpc_addr = 'http:/{{ fetch_ip }}:15257'
grpc_addr = 'http:/{{ fetch_ip }}:15290'
websocket_addr = 'ws:/{{ fetch_ip }}:15257/websocket'

rpc_timeout = '20s'
account_prefix = 'fetch'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 300000
max_gas =  1000000
gas_price = { price = 20000000000, denom = 'afet' }
gas_multiplier = 1.4
max_msg_num = 30
max_tx_size = 250000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '14days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', 'channel-14'], # Axelar
]