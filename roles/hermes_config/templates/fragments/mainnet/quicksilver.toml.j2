[[chains]]
id = 'quicksilver-2'
rpc_addr = 'http://{{ quicksilver_ip }}:11157'
grpc_addr = 'http://{{ quicksilver_ip }}:11190'
websocket_addr = 'ws://{{ quicksilver_ip }}:11157/websocket'

rpc_timeout = '20s'
account_prefix = 'quick'
key_name = '{{ key_name }}'
store_prefix = 'ibc'
address_type = { derivation = 'cosmos' }
default_gas = 400000
max_gas = 120000000
gas_price = { price = 0.0001, denom = 'uqck' }
gas_multiplier = 1.2
max_msg_num = 30
max_tx_size = 1800000
clock_drift = '15s'
max_block_time = '10s'
trusting_period = '14days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }

[chains.packet_filter]
policy = 'allow'
list = [
    ['transfer', 'channel-7'], # Evmos
    ['transfer', 'channel-0'], # Stargaze
    ['transfer', 'channel-1'], # Cosmos
    ['transfer', 'channel-2'], # Osmosis
    ['transfer', 'channel-16'], # Persistence
    ['icacontroller-stargaze-1.deposit', 'channel-3'],
    ['icacontroller-stargaze-1.withdrawal', 'channel-4'],
    ['icacontroller-stargaze-1.performance', 'channel-5'],
    ['icacontroller-stargaze-1.delegate', 'channel-6'],
]
