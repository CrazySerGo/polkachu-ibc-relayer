[[chains]]
id = 'phoenix-1'
rpc_addr = 'http://{{ terra2_ip }}:26657'
grpc_addr = 'http://{{ terra2_ip }}:11790'
websocket_addr = 'ws://{{ terra2_ip }}:26657/websocket'

rpc_timeout = '20s'
account_prefix = 'terra'
key_name = '{{ key_name }}'
address_type = { derivation = 'cosmos' }
store_prefix = 'ibc'
default_gas = 300000
max_gas = 3500000
gas_price = { price = 0.015, denom = 'uluna' }
gas_multiplier = 1.3
max_msg_num = 30
max_tx_size = 100000
clock_drift = '45s'
max_block_time = '10s'
trusting_period = '14days'
memo_prefix = '{{ memo }}'
trust_threshold = { numerator = '1', denominator = '3' }


